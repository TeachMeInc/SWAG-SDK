{
  "name": "html5",
  "private": true,
  "version": "5.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/fix-icon-paths.js",
    "build:staging": "tsc && vite build --mode staging && node scripts/fix-icon-paths.js",
    "build:demo": "tsc && vite build --config vite.config.demo.ts && node scripts/fix-icon-paths.demo.js",
    "build:types:bundle": "rollup -c rollup.dts.config.js",
    "build:types:global": "node scripts/make-global-dts.js",
    "build:types": "npm run build:types:bundle && npm run build:types:global",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-web": "^0.45.0",
    "@lottielab/lottie-player": "^1.1.3",
    "axios": "^1.7.0",
    "lean-qr": "^2.5.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.3",
    "@sumsolution/vite-plugin-svg-to-font": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "rollup": "^4.52.3",
    "rollup-plugin-dts": "^6.2.3",
    "sass": "^1.77.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-mkcert": "^1.17.5"
  }
}
